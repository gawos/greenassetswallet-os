{
  "name": "gaw-investor",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "preinstall": "npm run copy-form-builder && npm run symlink-graphql",
    "rm-form-builder": "rm -rf ./src/containers/FormBuilder",
    "copy-form-builder": "npm run rm-form-builder && cp -rf ../shared/FormBuilder ./src/containers",
    "symlink-graphql": "ln -fs $(dirname `pwd`)/shared/queries/ ./src",
    "lint": "eslint **/*.js --ignore-pattern node_modules/",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "postbuild": "mkdir -p ../build/investor && cp -f -H -R build/* ../build/investor && rm -r build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": ""
  },
  "dependencies": {
    "antd": "^3.10.1",
    "apollo-cache-inmemory": "^1.2.1",
    "apollo-client": "^2.4.2",
    "apollo-link": "^1.2.2",
    "apollo-upload-client": "^9.1.0",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.9.2",
    "jwt-decode": "^2.2.0",
    "mobx": "^5.0.3",
    "mobx-apollo": "0.0.18",
    "mobx-persist": "^0.4.1",
    "mobx-react": "^5.3.4",
    "mobx-utils": "^5.0.0",
    "moment": "^2.22.1",
    "qs": "^6.5.1",
    "react": "^16.5.2",
    "react-apollo": "^2.3.1",
    "react-app-rewire-graphql-tag": "^1.1.0",
    "react-dom": "^16.5.2",
    "react-router-dom": "^4.3.1",
    "recharts": "^1.3.4",
    "sass-loader": "^7.0.2",
    "transform-props-with": "^2.2.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "apollo-link-context": "^1.0.8",
    "apollo-link-error": "^1.1.0",
    "babel-plugin-inline-import-graphql-ast": "^2.4.1",
    "css-loader": "^1.0.0",
    "eslint-config-problems": "^1.1.0",
    "eslint-plugin-react": "^7.11.1",
    "node-css": "^0.1.0",
    "node-sass": "^4.9.0",
    "react-app-rewire-antd": "^1.0.3",
    "react-app-rewire-css-modules": "github:codebandits/react-app-rewire-css-modules",
    "react-app-rewire-graphql-tag": "^1.1.0",
    "react-app-rewire-mobx": "^1.0.8",
    "react-app-rewired": "^1.6.2",
    "react-scripts": "^1.1.5",
    "sass-loader": "^7.0.2"
  }
}
